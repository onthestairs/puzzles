        },
        "description": "This is an API that tests swagger integration"
    },
    "paths": {
        "/users": {
            "get": {
                "operationId": "getUsers",
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json;charset=utf-8": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    },
                                    "type": "array"
                                }
                            },
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    },
                                    "type": "array"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "User": {
                "required": [
                    "name",
                    "age",
                    "email",
                    "registration_date"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "registration_date": {
                        "$ref": "#/components/schemas/Day"
                    },
                    "age": {
                        "maximum": 9223372036854776000,
                        "minimum": -9223372036854776000,
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "Day": {
                "example": "2016-07-22",
                "format": "date",
                "type": "string"
            }
        }
    }
}
